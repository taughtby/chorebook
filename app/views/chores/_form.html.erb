<%= form_for @chore, remote: true do |f| %>
  <%= f.text_field :name, placeholder: "Chore description" %>
  <%= f.number_field :points, placeholder: "Points", default: 50 %>
  <%= f.number_field :child_id, placeholder: "Child_id" %>
  <%# f.date_select :due_date, placeholder: "Due Date"%>
  <%= f.submit %>
<% end %>

